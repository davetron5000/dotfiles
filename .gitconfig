[user]
  name = Dave Copeland
  email = david.copeland@livingsocial.com
[core]
  excludesfile = /Users/davec/.gitignore
[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = green reverse
  local = green
  remote = yellow
[color "diff"]
  meta = yellow
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow bold
  changed = red bold
  untracked = cyan
[alias]
  lol = log --oneline --graph --decorate
[rerere]
  enabled = 1
